<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>BMI_Client</servlet-name>
        <servlet-class>com.cal.BMI_Client</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CBR_Client</servlet-name>
        <servlet-class>com.cal.CBR_Client</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ABSI_Client</servlet-name>
        <servlet-class>com.cal.ABSI_Client</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BF_Client</servlet-name>
        <servlet-class>com.cal.BF_Client</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BMI_Client</servlet-name>
        <url-pattern>/BMI_Client</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CBR_Client</servlet-name>
        <url-pattern>/CBR_Client</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ABSI_Client</servlet-name>
        <url-pattern>/ABSI_Client</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BF_Client</servlet-name>
        <url-pattern>/BF_Client</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
